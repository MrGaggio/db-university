SELECT COUNT(`id`), YEAR(`enrolment_date`) AS `year`
FROM `students`
GROUP BY `year`;

SELECT COUNT(`id`), `office_address` 
FROM `teachers` 
GROUP BY `office_address`;

SELECT AVG(`vote`), `exam_id`
FROM `exam_student`
GROUP BY `exam_id`;

SELECT COUNT(`id`), `department_id`
FROM `degrees`
GROUP BY `department_id`;